{
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "declaration": true,
    "noImplicitOverride": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "useUnknownInCatchVariables": false
  }
}
